<div class="article">
    <h1>Article</h1>
    </br>
    {{#if detailVip}}
        {{#each detailVip}}
            <b>{{this.VIP_PRENOM}} {{this.VIP_NOM}}</b>
            </br></br>
        {{/each}}
    {{/if}}
    {{#if article}}
        <div class="articleOk">
            {{#each article}}
                {{this.article}}
                </br></br>
                [Ajout√© le {{formatDate this.ARTICLE_DATE_INSERT 'dddd D MMMM YYYY'}}] - <a
                    href="/repertoire/vip/{{this.VIP_NUMERO}}"> Voir sa fiche. </a>
                </br></br>
            {{/each}}
        </div>
    {{else}}
        <p>Il n'y a pas encore d'article sur ce VIP.</p>
    {{/if}}
    </br>
</div>

<div class="listeAllVip">
    {{#if allVip}}
        <h1>VIPs</h1>
        {{#each allVip}}
            <a href="/articles/{{this.VIP_NUMERO}}">{{this.VIP_NOM}} {{this.VIP_PRENOM}}</a> </br>
        {{/each}}
    {{/if}}
</div>
</br></br>

